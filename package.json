{
  "name": "@aeternity/aepp-calldata",
  "description": "Aeternity data serialization library",
  "version": "0.1.0",
  "author": "aeternity",
  "license": "ISC",
  "homepage": "https://github.com/aeternity/aepp-calldata-js#readme",
  "bugs": {
    "url": "https://github.com/aeternity/aepp-calldata-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeternity/aepp-calldata-js.git"
  },
  "keywords": [
    "aesophia",
    "aeternity",
    "calldata",
    "decoder",
    "encoder",
    "fate",
    "serialization"
  ],
  "main": "src/Encoder.js",
  "files": [
    "src/*"
  ],
  "dependencies": {
    "base64check": "^2.1.3",
    "blakejs": "^1.1.0",
    "bs58check": "^2.1.2",
    "rlp": "^2.2.4"
  },
  "devDependencies": {
    "ava": "^3.1.0",
    "browserify": "^16.5.2",
    "nyc": "^15.0.0",
    "tape": "^5.0.1"
  },
  "ava": {
    "files": [
      "tests/**/*",
      "!tests/test.js"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "temp-dir": "build/.nyc_output"
  },
  "scripts": {
    "browser-test-bundle": "browserify tests/**/*.js > build/js/tests.js",
    "coverage": "nyc ava",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch"
  }
}
